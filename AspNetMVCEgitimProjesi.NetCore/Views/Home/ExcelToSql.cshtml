
@{
    ViewData["Title"] = "ExcelToSql";
}

<h1>ExcelToSql</h1>

<form action="" method="post" enctype="multipart/form-data">
    <input type="file" name="excelFile" />
    <input type="submit" value="Yükle" />
</form>

@if (ViewBag.Mes is not null)
{
    <table>
        @foreach (var item in ViewBag.Mes as List<string>)
        {
            <tr>
                <td>@item</td>
            </tr>
        }
    </table>
}
